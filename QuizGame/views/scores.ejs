<%-include("partials/header")%>
        <% for (const score of scores) { %>
            <section class="scoreContainer">
                <section class="scoreResult">
                    <div>
                        <h1 class="testTitle"><%=score.title%></h1>
                        <p><%=score.desc%></p>
                    </div>
                    <div>
                        <h1 class="score" style="-webkit-text-stroke: 1px <%=score.score > 70 ? 'green' : 'darkred'%>"><%=score.score%>%</h1>
                    </div>
                </section>
                <hr>
            </section>
        <% } %>
    </main>
    <script src="js/scores.js"></script>
</body>
</html>